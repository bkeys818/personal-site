{
    "extends": [
        "eslint:recommended",
        "plugin:astro/recommended",
        "plugin:jsx-a11y/recommended",
        "prettier"
    ],
    "plugins": ["jsx-a11y"],
    "overrides": [
        {
            "files": ["*.astro"],
            "parser": "astro-eslint-parser",
            "parserOptions": {
                "parser": "@typescript-eslint/parser",
                "extraFileExtensions": [".astro"]
            }
        }
    ]
}
